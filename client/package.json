{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -p dev:nextjs prettier:watch stylelint:watch",
    "dev:nextjs": "next dev",
    "watch": "npm-run-all -p prettier:watch stylelint:watch",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "next start",
    "prettier:watch": "onchange '**/*.tsx' '**/*.js' -- prettier --write {{changed}}",
    "stylelint:watch": "onchange '**/styled.tsx' -- stylelint --fix {{changed}}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "graphql": "^14.5.8",
    "next": "11.1.3",
    "node-fetch": "^2.6.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@storybook/addon-actions": "^5.3.6",
    "@storybook/addon-links": "^5.3.6",
    "@storybook/addons": "^5.3.6",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.3.6",
    "@types/node": "^13.1.4",
    "@types/react": "^16.9.17",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "storybook-addon-styled-component-theme": "^1.3.0",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.0.0",
    "stylelint-processor-styled-components": "^1.9.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.4"
  }
}
